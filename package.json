{
  "name": "truebase",
  "version": "4.2.0",
  "description": "TrueBase is a system for long-term, collaborative, trustworthy semantic knowledge bases",
  "author": "Breck Yunits et al",
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "semi": false,
    "printWidth": 240,
    "trailingComma": "none"
  },
  "scripts": {
    "build": "tsc",
    "test": "./test.js",
    "cloc": "cloc $(git ls-files | grep --extended-regexp --invert-match 'products|lib|package-lock')"
  },
  "files": [
    "code/*",
    "tql/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/breck7/truebase"
  },
  "dependencies": {
    "express": "*",
    "jtree": "^69.2.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "scroll-cli": "^58.2.3"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.74",
    "@types/node": "^11.15.54",
    "ts-node": "^8.10.2",
    "typescript": "^4.9.5"
  }
}
